@model System.Data.DataTable



@{
    ViewBag.Title = "Index";
}

<a href="@Url.Action("Create","PregledPoDatumu")">Odaberite Datume</a>
<hr />

<table class="table table-bordered table-striped">
    <tr>
        <th>Datum</th>
        <th>Ime Tijela</th>
        <th>Ime Proizvoda</th>
        <th>Rezultat</th>
        <th>Sigurnost</th>


    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][4]</td>
            <td>@Model.Rows[i][5]</td>



        </tr>
    }
    </table>
    <br />
    <a href="@Url.Action("Index","Home")">Povratak na početnu</a>
